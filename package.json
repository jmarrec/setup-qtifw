{
  "name": "setup-qtifw",
  "version": "1.0.0",
  "private": true,
  "description": "Setup QtIFW action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --coverage --testPathPattern=main --detectOpenHandles",
    "test-integration": "jest --testPathPattern=integration",
    "test-all": "jest --coverage",
    "all": "npm run build && npm run format && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmarrec/setup-qtifw.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "C++",
    "Qt",
    "QtIFW"
  ],
  "author": "Julien Marrec, EffiBEM",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/tool-cache": "^2.0.1",
    "axios": "^1.5.0",
    "cheerio": "*",
    "semver": "^7.5.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7 ",
    "@types/semver": "^7.5.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/parser": "^6.4.1",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.48.0",
    "eslint-plugin-github": "^4.9.2",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.6.4",
    "jest-circus": "^29.6.4",
    "js-yaml": "^4.1.0",
    "prettier": "3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
