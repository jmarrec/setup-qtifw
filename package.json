{
  "name": "setup-qtifw",
  "version": "1.0.0",
  "private": true,
  "description": "Setup QtIFW action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --coverage --testPathPattern=main --detectOpenHandles",
    "test-integration": "jest --testPathPattern=integration",
    "test-all": "jest --coverage",
    "all": "npm run build && npm run format && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmarrec/setup-qtifw.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "C++",
    "Qt",
    "QtIFW"
  ],
  "author": "Julien Marrec, EffiBEM",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/tool-cache": "^1.7.1",
    "axios": "^1.1.0",
    "cheerio": "*",
    "semver": "^7.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.30",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/semver": "^7.3.6",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/parser": "^5.4.0",
    "@vercel/ncc": "^0.32.0",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^25.2.4",
    "jest": "^26.6.3",
    "jest-circus": "^27.3.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.4.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.4.4"
  }
}
